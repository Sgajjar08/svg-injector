{
  "name": "@tanem/svg-injector",
  "version": "3.0.0",
  "description": "Fast, caching, dynamic inline SVG DOM injection library.",
  "author": {
    "name": "Iconic",
    "email": "yourfriends@useiconic.com",
    "url": "https://useiconic.com/"
  },
  "main": "index.js",
  "module": "es/svg-injector.js",
  "jsnext:main": "es/svg-injector.js",
  "typings": "svg-injector.d.ts",
  "files": [
    "cjs",
    "es",
    "svg-injector.d.ts",
    "umd"
  ],
  "homepage": "https://github.com/tanem/svg-injector",
  "bugs": "https://github.com/tanem/svg-injector/issues",
  "repository": "github:tanem/svg-injector",
  "keywords": [
    "SVG",
    "Scalable Vector Graphics",
    "SVG injector",
    "images",
    "img",
    "html",
    "DOM"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-typescript": "^7.1.0",
    "@types/chai": "^4.1.7",
    "@types/karma": "^3.0.0",
    "@types/karma-chai": "^0.1.1",
    "@types/mocha": "^5.2.5",
    "@types/prettier": "^1.15.2",
    "@types/sinon": "^5.0.7",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "del-cli": "^1.1.0",
    "karma": "^3.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "^3.0.13",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^6.0.0",
    "shelljs": "^0.8.3",
    "shelljs-plugin-authors": "^2.0.6",
    "sinon": "^7.1.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  },
  "scripts": {
    "authors": "shelljs-authors > AUTHORS",
    "build": "run-s clean compile bundle",
    "bundle": "rollup -c",
    "changelog": "github_changelog_generator --no-issues --header-label \"# Changelog\" --future-release v$npm_package_version",
    "check:format": "prettier --list-different \"**/*.{js,ts}\"",
    "check:types": "tsc --noEmit",
    "clean": "run-p clean:*",
    "clean:cjs": "del cjs",
    "clean:coverage": "del coverage",
    "clean:es": "del es",
    "clean:typings": "del svg-injector.d.ts",
    "clean:umd": "del umd",
    "compile": "tsc --emitDeclarationOnly",
    "format": "prettier --write \"**/*.{js,ts}\"",
    "lint": "tslint -t stylish -p .",
    "postcompile": "del '*.d.ts' '!svg-injector.d.ts'",
    "postversion": "git push && git push --tags && npm publish --access=public",
    "release": "npm version -m 'Release v%s'",
    "test:karma": "karma start",
    "test": "run-s check:* lint build test:karma",
    "version": "run-s test changelog authors && git add ."
  }
}

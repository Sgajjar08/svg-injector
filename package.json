  "files": [
    "esm",
    "lib",
    "umd"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@types/chai": "^4.1.7",
    "@types/karma": "^3.0.0",
    "@types/karma-chai": "^0.1.1",
    "@types/mocha": "^5.2.5",
    "@types/prettier": "^1.15.2",
    "@types/sinon": "^5.0.7",
    "babel-eslint": "^8.2.3",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.1.6",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "karma": "^3.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-mocha": "^1.3.0",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "^3.0.13",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.59.3",
    "rollup-plugin-babel": "^4.0.0-beta.2",
    "rollup-plugin-uglify": "^4.0.0",
    "sinon": "^7.1.1",
    "typescript": "^3.1.6",
    "uglify-js": "^2.4.13"
  },
  "scripts": {
    "build:commonjs": "cross-env BABEL_OUTPUT=commonjs babel src -d lib",
    "build:esm": "babel src -d esm",
    "build:umd": "rollup -c",
    "clean": "rimraf esm lib umd",
    "format": "prettier --write src/*.js",
    "format:check": "prettier --list-different src/*.js",
    "lint": "eslint .",
    "postversion": "git push && git push --tags && npm publish --access=public",
    "release": "npm version -m 'Release v%s'",
    "test": "karma start",
    "version": "for TASK in format:check lint clean build:commonjs build:esm build:umd; do npm run $TASK; done"
  }
}
